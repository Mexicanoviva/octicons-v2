{
  "name": "Octicons Viewer",
  "description": "Preview new Octicons on github.com",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["build/bundle.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    }
  }
}
