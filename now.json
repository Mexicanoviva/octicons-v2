{
  "version": 2,
  "name": "octicons-v2",
  "alias": ["octicons-v2", "octicons-viewer"],
  "builds": [
    {
      "src": "site/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    },
    { "src": "og-image/index.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/og-image", "dest": "/og-image/index.ts" },
    { "src": "/(.*)", "dest": "/site/$1" }
  ]
}
